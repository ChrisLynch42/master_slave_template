<h1>Listing feats</h1>

<table>
  <tr>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @feats.each do |feat| %>
  <tr>
    <td><%= feat.title %></td>
    <td><%= link_to 'Show', feat, :class => 'ajaxLink'  %></td>
    <td><%= link_to 'Edit', edit_feat_path(feat), :class => 'ajaxLink' %></td>
    <td><%= link_to 'Destroy', feat_path(), :class => 'ajaxDeleteLink' %></td>  
  </tr>
<% end %>
</table>

<br />
<%= link_to 'New Feat', new_feat_path(), :class => 'ajaxLink' %>
